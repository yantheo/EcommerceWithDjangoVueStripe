{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: \"Product\",\n  data() {\n    return {\n      product: {},\n      quantity: 1\n    };\n  },\n  mounted() {\n    this.getProduct();\n  },\n  methods: {\n    async getProduct() {\n      this.$store.commit('setIsLoading', true);\n      const category_slug = this.$route.params.category_slug;\n      const product_slug = this.$route.params.product_slug;\n      await axios.get(`/api/v1/products/${category_slug}/${product_slug}`).then(response => {\n        this.product = response.data;\n        document.title = this.product;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.$store.commit('setIsLoading', false);\n    },\n    addToCart() {\n      //console.log('addToCart')\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n      const item = {\n        product: this.product,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'The product was added to the cart',\n        type: 'is-success',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","toast","name","data","product","quantity","mounted","getProduct","methods","$store","commit","category_slug","$route","params","product_slug","get","then","response","document","title","catch","error","console","log","addToCart","isNaN","item","message","type","dismissible","pauseOnHover","duration","position"],"sources":["C:\\Users\\utilisateur\\Desktop\\Web Development\\Python\\django_vuejs\\vue_djakets\\src\\views\\Product.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-product\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-9\">\r\n        <figure class=\"image mb-6\">\r\n          <img :src=\"product.get_image\" :alt=\"product.name\" style=\"width: 100%; height: 337px; object-fit: contain;\">\r\n        </figure>\r\n        <h1 class=\"title\">{{ product.name }}</h1>\r\n        <p>{{ product.description }}</p>\r\n      </div>\r\n      <div class=\"column is-3\">\r\n        <h2 class=\"subtitle\">Information</h2>\r\n        <p><strong>Price: </strong>${{ product.price }}</p>\r\n        <div class=\"field has-addons mt-6\">\r\n          <div class=\"control\">\r\n            <input type=\"number\" class=\"input\" min=\"1\" v-model=\"quantity\">\r\n          </div>\r\n          <div class=\"control\">\r\n            <button class=\"button is-dark\" @click=\"addToCart\">Add to cart</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nimport {toast} from 'bulma-toast'\r\n\r\nexport default {\r\n  name: \"Product\",\r\n  data(){\r\n    return {\r\n      product:{},\r\n      quantity:1\r\n    }\r\n  },\r\n  mounted(){\r\n    this.getProduct()\r\n  },\r\n  methods: {\r\n    async getProduct(){\r\n      this.$store.commit('setIsLoading', true)\r\n      const category_slug = this.$route.params.category_slug\r\n      const product_slug = this.$route.params.product_slug\r\n      await axios\r\n        .get(`/api/v1/products/${category_slug}/${product_slug}`)\r\n        .then(response => {\r\n          this.product = response.data\r\n          document.title = this.product\r\n        })\r\n        .catch(error => {\r\n          console.log(error)\r\n        })\r\n        this.$store.commit('setIsLoading', false)\r\n    },\r\n    addToCart(){\r\n      //console.log('addToCart')\r\n      if (isNaN(this.quantity) || this.quantity < 1 ){\r\n        this.quantity = 1\r\n      }\r\n      const item = {\r\n        product: this.product,\r\n        quantity: this.quantity\r\n      }\r\n      this.$store.commit('addToCart', item)\r\n      toast({\r\n        message: 'The product was added to the cart',\r\n        type: 'is-success',\r\n        dismissible: true,\r\n        pauseOnHover: true,\r\n        duration: 2000,\r\n        position: 'bottom-right',\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AA0BA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAAQC,KAAK,QAAO,aAAY;AAEhC,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,OAAO,EAAC,CAAC,CAAC;MACVC,QAAQ,EAAC;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,UAAU,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAE;MAChB,IAAI,CAACE,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,IAAI;MACvC,MAAMC,aAAY,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,aAAY;MACrD,MAAMG,YAAW,GAAI,IAAI,CAACF,MAAM,CAACC,MAAM,CAACC,YAAW;MACnD,MAAMd,KAAI,CACPe,GAAG,CAAC,oBAAoBJ,aAAa,IAAIG,YAAY,EAAE,EACvDE,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACb,OAAM,GAAIa,QAAQ,CAACd,IAAG;QAC3Be,QAAQ,CAACC,KAAI,GAAI,IAAI,CAACf,OAAM;MAC9B,CAAC,EACAgB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB,CAAC;MACD,IAAI,CAACZ,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE,KAAK;IAC5C,CAAC;IACDc,SAASA,CAAA,EAAE;MACT;MACA,IAAIC,KAAK,CAAC,IAAI,CAACpB,QAAQ,KAAK,IAAI,CAACA,QAAO,GAAI,GAAG;QAC7C,IAAI,CAACA,QAAO,GAAI;MAClB;MACA,MAAMqB,IAAG,GAAI;QACXtB,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,QAAQ,EAAE,IAAI,CAACA;MACjB;MACA,IAAI,CAACI,MAAM,CAACC,MAAM,CAAC,WAAW,EAAEgB,IAAI;MACpCzB,KAAK,CAAC;QACJ0B,OAAO,EAAE,mCAAmC;QAC5CC,IAAI,EAAE,YAAY;QAClBC,WAAW,EAAE,IAAI;QACjBC,YAAY,EAAE,IAAI;QAClBC,QAAQ,EAAE,IAAI;QACdC,QAAQ,EAAE;MACZ,CAAC;IACH;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}